{
  "name": "@desterlib/mpv",
  "version": "0.34.6",
  "description": "Dester's mpv video plugin",
  "license": "MIT",
  "scripts": {
    "install" : "echo",
    "demo": "electronmon demo",
    "build:pre": "rd /s /q dist || exit 0",
    "build": "npm run build:pre && node-gyp rebuild && npm run build:post",
    "build:post": "robocopy libraries/libmpv/win build/Release mpv-1.dll || exit 0",
    "release:pre": "robocopy build/Release dist mpv-1.dll mpvjs.node || exit 0",
    "release": "npm run build && npm run release:pre && npm publish"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "electron": "^3.1.13",
    "electronmon": "^2.0.2",
    "node-abi": "^3.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "contributors": [
    {
      "name": "Elias Benbourenane",
      "email": "me@elias.eu.org",
      "url": "https://elias.eu.org"
    },
    {
      "name": "Kagami Hiiragi",
      "url": "https://github.com/Kagami"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DesterLib/mpv.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js"
}
