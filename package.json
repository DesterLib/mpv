{
  "name": "@desterlib/mpv",
  "version": "0.34.0",
  "description": "Dester's mpv video plugin",
  "license": "MIT",
  "scripts": {
    "install": "npm run build",
    "demo": "electronmon demo",
    "build:pre": "rd /s /q prebuilds || exit 0",
    "build": "npm run build:pre && prebuild -t 93 -a x64 && npm run build:post",
    "build:post": "robocopy libraries/libmpv build/Release mpv-1.dll || exit 0",
    "publish:pre": "mkdir dist && robocopy build/Release dist mpv-1.dll mpvjs.node || exit 0",
    "publish": "npm run build && npm run publish:pre && npm publish"
  },
  "dependencies": {
    "prebuild-install": "^5.0.0",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "babel-standalone": "^6.26.0",
    "electron": "^3.1.13",
    "electronmon": "^2.0.2",
    "node-abi": "^3.22.0",
    "prebuild": "^10.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "contributors": [
    {
      "name": "Elias Benbourenane",
      "email": "me@elias.eu.org",
      "url": "https://elias.eu.org"
    },
    {
      "name": "Kagami Hiiragi",
      "url": "https://github.com/Kagami"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DesterLib/mpv.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "_from": ""
}
